/**
 * The Main class initializes a new SpeedTyping game and starts it.
 */
class Main {

    /** Initializes a new game and starts it. */    
    function void main() {
        var SpeedTypingGame game;

        let game = SpeedTypingGame.new();
        do game.run();
        do game.dispose();

        return;
    }
}
