class Node {
    field Node next;
    field Position pos;

    constructor Node new(Position item, Node nextNode) {
        let next = nextNode;
        let pos = item;
        return this;
    }

    method void dispose() {
        do pos.dispose();
        return;
    }

    method Position getItem() {
        return pos;
    }

    method Node nextNode() {
        return next;
    }
}
